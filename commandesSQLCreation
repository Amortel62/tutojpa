CREATE TABLE PROJET (CODEPROJ VARCHAR(255) NOT NULL, DATEFINP DATE, MONTANTDEVIS FLOAT, DESCPROJ VARCHAR(255), DATEDEP DATE, PRIMARY KEY (CODEPROJ))
CREATE TABLE SALARIE (NUMSAL BIGINT NOT NULL, NOMSAL VARCHAR(255), SEXE VARCHAR(255), SALAIRE FLOAT, CODEPOLE VARCHAR(255), PRIMARY KEY (NUMSAL))
CREATE TABLE POLE (CODEPOLE VARCHAR(255) NOT NULL, NOMPOLE VARCHAR(255), PRIMARY KEY (CODEPOLE))
CREATE TABLE CLIENT (NUMCLI BIGINT NOT NULL, NOMCLI VARCHAR(255), ADRCLI VARCHAR(255), CODEPROJ VARCHAR(255), PRIMARY KEY (NUMCLI))
ALTER TABLE SALARIE ADD CONSTRAINT SALARIE_CODEPOLE FOREIGN KEY (CODEPOLE) REFERENCES POLE (CODEPOLE)
ALTER TABLE CLIENT ADD CONSTRAINT FK_CLIENT_CODEPROJ FOREIGN KEY (CODEPROJ) REFERENCES PROJET (CODEPROJ)
