CREATE TABLE PROJET (CODEPROJ VARCHAR(255) NOT NULL, MONTANTDEVIS FLOAT, DATEFINP DATE, DESCPROJ VARCHAR(255), DATEDEP DATE, CODECLIENT BIGINT, PRIMARY KEY (CODEPROJ))
CREATE TABLE CLIENT (NUMCLI BIGINT NOT NULL, ADRCLI VARCHAR(255), NOMCLI VARCHAR(255), PRIMARY KEY (NUMCLI))
CREATE TABLE POLE (CODEPOLE VARCHAR(255) NOT NULL, NOMPOLE VARCHAR(255), PRIMARY KEY (CODEPOLE))
CREATE TABLE SALARIE (NUMSAL BIGINT NOT NULL, NOMSAL VARCHAR(255), SALAIRE FLOAT, SEXE VARCHAR(255), CODEPOLE VARCHAR(255), PRIMARY KEY (NUMSAL))
ALTER TABLE PROJET ADD CONSTRAINT PROJET_CODECLIENT FOREIGN KEY (CODECLIENT) REFERENCES CLIENT (NUMCLI)
ALTER TABLE SALARIE ADD CONSTRAINT SALARIE_CODEPOLE FOREIGN KEY (CODEPOLE) REFERENCES POLE (CODEPOLE)
